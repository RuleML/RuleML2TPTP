<?xml version="1.0" encoding="UTF-8"?>

<RuleML xmlns="http://ruleml.org/spec">
  <Assert>
    <Forall>
      <Var>person</Var>
      <Var>object</Var>
      <Equivalent>
        <torso>
          <Atom>
            <Rel>own</Rel>
            <Var>person</Var>
            <Var>object</Var>
          </Atom>
        </torso>
        <torso>
          <Atom>
            <Rel>belongs</Rel>
            <Var>object</Var>
            <Var>person</Var>
          </Atom>
        </torso>
      </Equivalent>
    </Forall>
  </Assert>
  <Query>
    <Equivalent>
      <torso>
        <Atom>
          <Rel>foo</Rel>
          <Ind>a</Ind>
        </Atom>
      </torso>
      <torso>
        <Equivalent>
          <torso>
            <Atom>
              <Rel>fop</Rel>
            </Atom>
          </torso>
          <torso>
            <Atom>
              <Rel>foq</Rel>
            </Atom>
          </torso>
        </Equivalent>
      </torso>
    </Equivalent>
  </Query>
</RuleML>
