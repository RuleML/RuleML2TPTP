<?xml version="1.0" encoding="UTF-8"?>

<RuleML xmlns="http://ruleml.org/spec">
  <Assert>
    <Neg>
      <Atom>
        <Rel>foo</Rel>
        <Ind>a</Ind>
      </Atom>
    </Neg>
  </Assert>
  <Query>
    <Neg>
      <Neg>
        <Atom>
          <Rel>foo</Rel>
          <Ind>a</Ind>
        </Atom>
      </Neg>
    </Neg>
  </Query>
  <Assert>
    <Implies>
      <Neg>
        <And>
          <Atom>
            <Rel>bar</Rel>
          </Atom>
          <Atom>
            <Rel>bax</Rel>
          </Atom>
        </And>
      </Neg>
      <Neg>
        <Atom>
          <Rel>bay</Rel>
        </Atom>
      </Neg>
    </Implies>
    <Forall>
      <Var>X</Var>
      <Neg>
        <Equivalent>
          <Atom>
            <Rel>foo</Rel>
            <Var>X</Var>
          </Atom>
          <Atom>
            <Rel>bar</Rel>
          </Atom>
        </Equivalent>
      </Neg>
    </Forall>
  </Assert>
  <Query>
    <Neg>
      <Exists>
        <Var>X</Var>
        <Equal>
          <Var>X</Var>
          <Atom>
            <Rel>bar</Rel>
          </Atom>
        </Equal>
      </Exists>
    </Neg>
  </Query>
</RuleML>
