<?xml version="1.0" encoding="UTF-8"?>

<RuleML xmlns="http://ruleml.org/spec">
  <Assert>
    <Or/>
    <Or>
      <Atom>
        <Rel>one</Rel>
      </Atom>
    </Or>
    <Or>
      <Atom>
        <Rel>one</Rel>
      </Atom>
      <Atom>
        <Rel>two</Rel>
      </Atom>
    </Or>
    <Or>
      <Atom>
        <Rel>one</Rel>
      </Atom>
      <Atom>
        <Rel>two</Rel>
      </Atom>
      <Atom>
        <Rel>three</Rel>
      </Atom>
    </Or>
    <Or>
      <Atom>
        <Rel>one</Rel>
      </Atom>
      <And>
        <Atom>
          <Rel>two</Rel>
        </Atom>
        <Atom>
          <Rel>three</Rel>
        </Atom>
      </And>
    </Or>
    <Forall>
      <Var>X</Var>
      <Or>
        <Atom>
          <Rel>foo</Rel>
          <Var>X</Var>
        </Atom>
      </Or>
    </Forall>
  </Assert>
  <Query>
    <Forall>
      <Var>X</Var>
      <Or>
        <Equal>
          <Var>X</Var>
          <Ind>ruleml</Ind>
        </Equal>
        <Atom>
          <Rel>foo</Rel>
        </Atom>
      </Or>
    </Forall>
    <Forall>
      <Var>X</Var>
      <Or>
        <Exists>
          <Var>Y</Var>
          <Equal>
            <Var>X</Var>
            <Var>Y</Var>
          </Equal>
        </Exists>
        <Atom>
          <Rel>foo</Rel>
          <Var>X</Var>
        </Atom>
      </Or>
    </Forall>
  </Query>
</RuleML>
